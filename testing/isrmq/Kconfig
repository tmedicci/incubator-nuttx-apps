config TESTING_ISRMQ
	tristate "ISR + MQ example"
	default n

if TESTING_ISRMQ

config TESTING_ISRMQ_PROGNAME
	string "Program name"
	default "isrmq"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config TESTING_ISRMQ_PRIORITY
	int "ISR + MQ task priority"
	default 100

config TESTING_ISRMQ_STACKSIZE
	int "ISR + MQ stack size"
	default DEFAULT_TASK_STACKSIZE

endif
